{
  "name": "vue-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --force",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
